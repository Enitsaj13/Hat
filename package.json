{
  "name": "hat",
  "main": "expo-router/entry",
  "version": "2.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios --device",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "npx expo prebuild --clean",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,json,md,ts,tsx}\" && prettier \"src/**/*.{js,jsx,json,md,ts,tsx}\" --check",
    "lint:fix": "eslint \"src/**/*.{js,jsx,json,md,ts,tsx}\" --fix && npm run format",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "build:ios:sim": "eas build --profile preview --platform ios --local",
    "build:ios:dev": "eas build --profile preview --platform ios --local",
    "build:ios:qas": "NODE_ENV=qas eas build --profile preview --platform ios --local",
    "build:ios:prod": "NODE_ENV=prod eas build --profile production --platform ios --local",
    "build:android:sim": "eas build --profile preview --platform android --local",
    "build:android:dev": "eas build --profile preview --platform android --local",
    "build:android:qas": "NODE_ENV=qas eas build --profile preview --platform android --local",
    "build:android:prod": "NODE_ENV=prod eas build --profile production --platform android --local",
    "bundle:web": "npx expo export --platform web",
    "serve:web": "npx server dist",
    "upgrade-deps": "npx expo install --check",
    "upgrade-expo": "npm install expo@latest && npx expo install --check",
    "prepare": "husky install"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^4",
    "@hookform/resolvers": "^3.3.4",
    "@morrowdigital/watermelondb-expo-plugin": "^2.3.1",
    "@nozbe/watermelondb": "^0.27.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-navigation/native": "^6.0.2",
    "async-mutex": "^0.5.0",
    "axios": "^1.6.8",
    "dayjs": "^1.11.11",
    "deepmerge": "^4.3.1",
    "expo": "^51.0.8",
    "expo-build-properties": "~0.12.1",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.14",
    "expo-font": "~12.0.5",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-router": "~3.5.14",
    "expo-secure-store": "~13.0.1",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-updates": "~0.25.14",
    "expo-web-browser": "~13.0.3",
    "husky": "^9.0.11",
    "i18n-js": "^4.4.3",
    "install": "^0.13.0",
    "lodash.clonedeep": "^4.0.8",
    "lodash.isempty": "^4.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.1",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-unistyles": "^2.5.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.8.6",
    "react-query": "^3.39.3",
    "use-between": "^1.3.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/lodash.clonedeep": "^4.0.9",
    "@types/lodash.isempty": "^4.4.9",
    "@types/react": "~18.2.79",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.2",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
    "snyk": "^1.1284.0",
    "typescript": "~5.3.3"
  },
  "overrides": {
    "@expo/image-utils": {
      "semver": "7.6.0"
    }
  },
  "private": true
}
